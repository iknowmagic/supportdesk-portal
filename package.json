{
  "name": "noctare",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "./scripts/start-dev-environment.zsh",
    "build": "tsc -b && vite build",
    "lint:tsc": "tsc --noEmit --project tsconfig.app.json",
    "lint:eslint": "eslint .",
    "lint": "eslint . && tsc --noEmit --project tsconfig.app.json && pnpm deno-check",
    "lint:file": "pnpm lint > tmp/lint-errors.txt 2>&1",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "shadcn-add": "pnpm dlx shadcn@latest add",
    "test": "vitest run",
    "test:file": "sh ./scripts/run-test-suite-json.zsh",
    "test:file-verbose": "sh ./scripts/run-test-suite-verbose.zsh",
    "test:failing-files": "sh ./scripts/extract-failing-test-files.zsh",
    "test:single": "sh ./scripts/run-single-test.zsh",
    "test:coverage": "vitest run --reporter=verbose --coverage",
    "test:coverage:summary": "pnpm test:coverage 2>&1 | awk -F'|' 'NF > 2 && $1 !~ /^-+/ && $1 != \"File\" { print $1, $2 }'",
    "file-list": "tree src supabase docs -I \"ui|shadcn-studio|migrations\"",
    "deno-check": "deno check --quiet supabase/functions/**/*.ts",
    "update-types": "pnpx supabase gen types typescript --local > src/types/supabase.ts",
    "db-dump": "pnpx supabase db dump --local > ./tmp/db-dump.txt",
    "knip": "knip",
    "e2e:single": "pnpm cypress run --spec",
    "duplicates:report": "pnpm jscpd ./src --pattern \"**/*.{ts,tsx}\" --ignore \"**/__tests__/**,**/*.test.{ts,tsx},**/*.spec.{ts,tsx},**/types/**\" --reporters json --output ./tmp --gitignore && jq 'del(.statistics, .total)' ./tmp/jscpd-report.json > ./tmp/jscpd-report.clean.json && mv ./tmp/jscpd-report.clean.json ./tmp/jscpd-report.json",
    "duplicates:terminal": "pnpm jscpd ./src  --pattern \"**/*.{ts,tsx}\" --ignore \"**/__tests__/**,**/*.test.{ts,tsx},**/*.spec.{ts,tsx},**/types/**\" --gitignore",
    "tree:code": "tree ./src ./supabase --dirsfirst --prune -I '__tests__|ui|assets|shadcn-studio|login-form.tsx|field-group.tsx|*.sql|*.json|*.toml'"
  },
